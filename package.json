{
  "name": "nextjs-blog-backend",
  "version": "1.0.0",
  "description": "Backend API for Next.js Blog CMS with AI workflows",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node server.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "migrate:pct-change": "tsx scripts/migrate-pct-change.ts",
    "update:min-pct-change": "tsx scripts/update-min-pct-change.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "docker:build": "./scripts/build-docker.sh",
    "docker:build:prod": "./scripts/build-docker.sh --tag production",
    "docker:build:dev": "./scripts/build-docker.sh --dev",
    "docker:push": "./scripts/build-docker.sh --push",
    "docker:build:multi": "./scripts/build-docker.sh --platform linux/amd64,linux/arm64",
    "docker:build:push": "./scripts/build-docker.sh --push",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/client": "^6.19.1",
    "@types/bcryptjs": "^2.4.6",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "litellm": "^0.12.0",
    "multer": "^2.0.2",
    "next-auth": "^4.24.13",
    "prisma": "^6.19.1",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwind-merge": "^3.4.0",
    "tsconfig-paths": "^4.2.0",
    "yahoo-finance2": "^3.11.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/multer": "^2.0.0",
    "@types/node": "^20",
    "@types/react": "^19.2.7",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9",
    "jest": "^29.7.0",
    "tsx": "^4.21.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
